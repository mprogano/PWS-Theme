//========== VELA - VARIABLES  ==========//
$vela-color-primary:            {{ settings.vela_color_primary }};
$vela-color-secondary:          {{ settings.vela_color_secondary }};
$vela-color-tertiary:           {{ settings.vela_color_tertiary }};
$vigor_color_red:               {{ settings.vigor_color_red }};
$vela-border-color:             {{ settings.vela_color_borders }};
$vela-body-bg:                  {{ settings.vela_color_background }};

$vela-text-color-primary:       {{ settings.text_color_primary }};
$vela-text-color-secondary:     {{ settings.text_color_secondary }};

$vela-border-main:              1px solid $vela-border-color;
$vela-gutter-width:             30px;

$vela-font-family:              {{ settings.vela_fontfamily }};
$vela-font-family-secondary:    {{ settings.vela_fontfamily_secondary }};
$vela-font-family-tertiary:     {{ settings.vela_fontfamily_tertiary }};
$vela-font-icon:                "FontAwesome";
$vela-font-icon-material  :      "Material-Design-Iconic-Font";
$font-themify          :             "themify";
$vela-font-size:                {{ settings.vela_fontsize }};
$nocolor:                       transparent;

/*================ SVG ================*/

$svg-select-icon: #{'{{ "ico-select.svg" | asset_url }}'};
  
/*========== BOOTSTRAP - VARIABLES  ==========*/
  
$gray-base:              #333333;
$gray-darker:            lighten($gray-base, 13.5%); // #222
$gray-dark:              lighten($gray-base, 20%);   // #333
$gray:                   lighten($gray-base, 33.5%); // #555
$gray-light:             lighten($gray-base, 46.7%); // #777
$gray-lighter:           lighten($gray-base, 93.5%); // #eee

$brand-primary:         $vela-color-primary; // #337ab7
$brand-success:         #5cb85c;
$brand-info:            #5bc0de;
$brand-warning:         #f0ad4e;
$brand-danger:          #d9534f;

$body-bg:               $vela-body-bg;
