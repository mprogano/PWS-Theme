  	}
    .iconText{
		display: none;
    }
  	.video-is-loaded & {
    	display: block;
    	visibility: visible;
    	opacity: 1;
  	}
  	.video-is-loading &,
  	.video-is-playing &,
  	.velassSlide.video-is-paused & {
