$velasubmenu-fontsize:          {{ settings.velamenu_dropdown_fontsize }};
#velaMegamenu{
    position:relative;
    margin:0px;
    width:100%;
    background-color: $velamenu-bgcolor;
    @media (max-width: 767px){
        .velaCartTop a>span.icon , .Topbar .titleGroup i{
			font-size:20px;
        }
    }
    .border_top{
		  border-top: 2px solid rgba(0, 0, 0, 0.1);
          padding-top: 0;
          margin-top: 0;
          @media (max-width: 767px){
				border:0px;
          }
    }
    .menuContainer{
        position:relative;
        margin:0px;
        padding:0px;
      	width: 100%;
        text-align: center;
      	
    }
}
.velaMegamenu{
    ul{list-style:none;padding:0px;}
    ul.nav{
        margin:0px;
        padding:0px;
      	display: inline-flex;
        >li{
            margin:0px;
            padding:0px;
            float:left;
          	text-align: left;
            .iconMenu{
              	color:$vela-color-primary;
                i{
                  	margin-right: 5px;
                }
                &:hover{
					color:$velamenu-linkcolor-hover;
                }
            }
            >a{
              	text-transform: uppercase;
    			letter-spacing: 0.5px;
                font-family:$vela-font-family-secondary;
                font-size:$velamenu-fontsize;
                font-weight:700;
                padding: 30px 20px 40px 20px;
                background-color:$nocolor;
                color:$velamenu-linkcolor;
                @include transition(all 0.35s ease);
                &:hover{
                    background-color:$nocolor;
                    color:$velamenu-linkcolor-hover;
                }
            }
            &.active{
                >a{
                    background-color:$nocolor;
                    color:$velamenu-linkcolor-hover;
                }
            }
            &.hasMenuDropdown{
                >a{
                    &:after{
                        content: "\f107";
                        @include inline-block;
                        font-family: $vela-font-icon;
                        float: right;
                        margin-left: 5px;
                    }
                }
                &.hasMegaMenu{
                    position:static;
                    &.menu-left{
						position:relative;
                        .menuDropdown {
							right:auto;
                          	left:0;
                        }
                    }
                  	&.menu-right{
						position:relative;
                        .menuDropdown {
							left:auto;
                          	right:0;
                        }
                    }
                    &.menu-default{
                      	.menuDropdown {
                         	 margin:auto;
                        }
                    }
                }
                &:hover,&:focus{
                    > .menuDropdown{
						display:block;
                    }
                }
            }
        }
    }
    .menuDropdown{
      	border-top: 0px solid $vela-color-primary;
        background-color: $velasubmenu-bgcolor;
        display:block;
        position:absolute;
        top:100%;
        left:0px;
        margin:0px;
        padding:30px 30px;
        min-width:220px;
        z-index:900;
		display:none;
        @include transition(all 0.15s ease);
      	@include box-shadow(0px 2px 6px rgba(0, 0, 0, 0.12));
        &.megaMenu{
            right:0px;
            width:100%;
        }
      	.menuTitle{
          	font-family:$vela-font-family-secondary;
        	font-weight:300;
          	font-size:$velasubmenu-fontsize + 2;
          	margin-bottom:10px;
          	color:$velasubmenu-linkcolor;
          	display:block;
          	text-transform: uppercase;
          	letter-spacing: 1px;
          	> span,a{
              	font-weight:700;
              	font-family:$vela-font-family-secondary;
                font-size:$velasubmenu-fontsize + 2;
                padding:0 0 0px 0px;
                background-color:$nocolor;
                color: inherit;
                display:block;
            }
          	&:after{
				font-family:$font-themify;
              	content: '\e706';
              	@include opacity(0.5);
            }
        }
        li{
            position:relative;
            border-bottom:1px dashed $vela-border-main;
            a{
                font-size:$velasubmenu-fontsize;
                font-weight:400;
                padding: 8px 0px;
                background-color:$nocolor;
                color:$vela-text-color-primary;
                display:block;
                @include transition(all 0.35s ease);
                &:hover{
                    background-color:$nocolor;
                    color:$velasubmenu-linkcolor-hover;
                    text-decoration:none;
                }
            }
            &.menuTitle{
            	margin-bottom:0px;
            }
            &.hasMenuDropdown{
                >a{
                    &:after{
                        content: "\f105";
                        @include inline-block;
                        font-family: $vela-font-icon;
                        float: right;
                        margin-left: 5px;
                    }
                }
                .menuDropdown{
                    top:0px;
                    left:100%;
                }
                &:hover,&:focus{
                    > .menuDropdown{
						display:block;
                    }
                }
            }
        }
    }
}
