.banner_effect{
    overflow: hidden;
  	position: relative;
  	display: block;
    &:before{
        position: absolute;
        content: " ";
        width: 92%;
        height: 92%;
        border: 3px solid white; 
        left: 4%;
        top: 4%;
        margin: auto;
        @include transition(all 0.55s ease);
        @include opacity(0);
        visibility: hidden;
    }
    &:after{
        position: absolute;
        content: " ";
        width: 92%;
        height: 92%;
        border: 1px solid white; 
        @include scale(0.7,0.7);
        margin: auto;
        left: 4%;
        top: 4%;
        @include opacity(0);
        visibility: hidden;
    }
    &:hover{
		&:before{
            @include opacity(1);
            visibility: visible;
        }
        &:after{
            @include transition(all 0.55s ease);
            @include scale(1,1);
            @include opacity(1);
            visibility: visible;
        }
    }
}
